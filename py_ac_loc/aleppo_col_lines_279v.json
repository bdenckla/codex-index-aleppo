  "column_1_lines": [
    {
      "line-num": 1,
      "range": {
        "start": "Job 35:10-mid",
        "end": "Job 35:11-mid"
      },
      "MAM-XML-fragment": "עֹשָׂ֑י נֹתֵ֖ן זְמִר֣וֹת בַּלָּֽיְלָה׃ מַ֭לְּפֵנוּ"
    },
    {
      "line-num": 2,
      "range": {
        "start": "Job 35:11-mid",
        "end": "Job 35:11-last"
      },
      "MAM-XML-fragment": "מִבַּהֲמ֣וֹת אָ֑רֶץ וּמֵע֖וֹף הַשָּׁמַ֣יִם יְחַכְּמֵֽנוּ׃"
    },
    {
      "line-num": 3,
      "range": {
        "start": "Job 35:12-first",
        "end": "Job 35:12-last"
      },
      "MAM-XML-fragment": "שָׁ֣ם יִ֭צְעֲקוּ וְלֹ֣א יַעֲנֶ֑ה מִ֝פְּנֵ֗י גְּא֣וֹן רָעִֽים׃"
    },
    {
      "line-num": 4,
      "range": {
        "start": "Job 35:13-first",
        "end": "Job 35:13-last"
      },
      "MAM-XML-fragment": "אַךְ־שָׁ֭וְא לֹא־יִשְׁמַ֥ע׀אֵ֑ל וְ֝שַׁדַּ֗י לֹ֣א יְשׁוּרֶֽנָּה׃"
    },
    {
      "line-num": 5,
      "range": {
        "start": "Job 35:14-first",
        "end": "Job 35:14-mid"
      },
      "MAM-XML-fragment": "אַ֣ף כִּֽי־תֹ֭אמַר לֹ֣א תְשׁוּרֶ֑נּוּ דִּ֥ין לְ֝פָנָ֗יו"
    },
    {
      "line-num": 6,
      "range": {
        "start": "Job 35:14-mid",
        "end": "Job 35:15-mid"
      },
      "MAM-XML-fragment": "וּתְח֥וֹלֵֽל לֽוֹ׃ וְעַתָּ֗ה כִּי־אַ֭יִן פָּקַ֣ד אַפּ֑וֹ וְלֹֽא־"
    },
    {
      "line-num": 7,
      "range": {
        "start": "Job 35:15-mid",
        "end": "Job 35:16-mid"
      },
      "MAM-XML-fragment": "יָדַ֖ע בַּפַּ֣שׁ מְאֹֽד׃ וְ֭אִיּוֹב הֶ֣בֶל יִפְצֶה־פִּ֑יהוּ"
    },
    {
      "line-num": 8,
      "range": {
        "start": "Job 35:16-mid",
        "end": "Job 35:16-last"
      },
      "MAM-XML-fragment": "בִּבְלִי־דַ֝֗עַת מִלִּ֥ין יַכְבִּֽר׃"
    },
    {
      "line-num": 9,
      "range": {
        "start": "Job 36:1-first",
        "end": "Job 36:1-last"
      },
      "MAM-XML-fragment": "וַיֹּ֥סֶף אֱלִיה֗וּא וַיֹּאמַֽר׃"
    },
    {
      "line-num": 10,
      "range": {
        "start": "Job 36:2-first",
        "end": "Job 36:2-last"
      },
      "MAM-XML-fragment": "כַּתַּר־לִ֣י זְ֭עֵיר וַאֲחַוֶּ֑ךָּ כִּ֤י ע֖וֹד לֶאֱל֣וֹהַּ מִלִּֽים׃"
    },
    {
      "line-num": 11,
      "range": {
        "start": "Job 36:3-first",
        "end": "Job 36:3-last"
      },
      "MAM-XML-fragment": "אֶשָּׂ֣א דֵ֭עִי לְמֵרָח֑וֹק וּ֝לְפֹעֲלִ֗י אֶֽתֵּֽן־צֶֽדֶק׃"
    },
    {
      "line-num": 12,
      "range": {
        "start": "Job 36:4-first",
        "end": "Job 36:4-last"
      },
      "MAM-XML-fragment": "כִּֽי־אׇ֭מְנָם לֹא־שֶׁ֣קֶר מִלָּ֑י תְּמִ֖ים דֵּע֣וֹת עִמָּֽךְ׃"
    },
    {
      "line-num": 13,
      "range": {
        "start": "Job 36:5-first",
        "end": "Job 36:5-last"
      },
      "MAM-XML-fragment": "הֶן־אֵ֣ל כַּ֭בִּיר וְלֹ֣א יִמְאָ֑ס כַּ֝בִּ֗יר כֹּ֣חַֽ לֵֽב׃"
    },
    {
      "line-num": 14,
      "range": {
        "start": "Job 36:6-first",
        "end": "Job 36:6-last"
      },
      "MAM-XML-fragment": "לֹא־יְחַיֶּ֥ה רָשָׁ֑ע וּמִשְׁפַּ֖ט עֲנִיִּ֣ים יִתֵּֽן׃"
    },
    {
      "line-num": 15,
      "range": {
        "start": "Job 36:7-first",
        "end": "Job 36:7-mid"
      },
      "MAM-XML-fragment": "לֹֽא־יִגְרַ֥ע מִצַּדִּ֗יק עֵ֫ינָ֥יו וְאֶת־מְלָכִ֥ים לַכִּסֵּ֑א"
    },
    {
      "line-num": 16,
      "range": {
        "start": "Job 36:7-mid",
        "end": "Job 36:8-mid"
      },
      "MAM-XML-fragment": "וַיֹּשִׁיבֵ֥ם לָ֝נֶ֗צַח וַיִּגְבָּֽהוּ׃ וְאִם־אֲסוּרִ֥ים בַּזִּקִּ֑ים"
    },
    {
      "line-num": 17,
      "range": {
        "start": "Job 36:8-mid",
        "end": "Job 36:9-mid"
      },
      "MAM-XML-fragment": "יִ֝לָּכְד֗וּן בְּחַבְלֵי־עֹֽנִי׃ וַיַּגֵּ֣ד לָהֶ֣ם פׇּעֳלָ֑ם"
    },
    {
      "line-num": 18,
      "range": {
        "start": "Job 36:9-mid",
        "end": "Job 36:10-mid"
      },
      "MAM-XML-fragment": "וּ֝פִשְׁעֵיהֶ֗ם כִּ֣י יִתְגַּבָּֽרוּ׃ וַיִּ֣גֶל אׇ֭זְנָם לַמּוּסָ֑ר"
    },
    {
      "line-num": 19,
      "range": {
        "start": "Job 36:10-mid",
        "end": "Job 36:11-mid"
      },
      "MAM-XML-fragment": "וַ֝יֹּ֗אמֶר כִּֽי־יְשׁוּב֥וּן מֵאָֽוֶן׃ אִֽם־יִשְׁמְע֗וּ"
    },
    {
      "line-num": 20,
      "range": {
        "start": "Job 36:11-mid",
        "end": "Job 36:11-last"
      },
      "MAM-XML-fragment": "וְֽיַ֫עֲבֹ֥דוּ יְכַלּ֣וּ יְמֵיהֶ֣ם בַּטּ֑וֹב וּ֝שְׁנֵיהֶ֗ם בַּנְּעִימִֽים׃"
    },
    {
      "line-num": 21,
      "range": {
        "start": "Job 36:12-first",
        "end": "Job 36:12-mid"
      },
      "MAM-XML-fragment": "וְאִם־לֹ֣א יִ֭שְׁמְעוּ בְּשֶׁ֣לַח יַעֲבֹ֑רוּ"
    },
    {
      "line-num": 22,
      "range": {
        "start": "Job 36:12-mid",
        "end": "Job 36:13-mid"
      },
      "MAM-XML-fragment": "וְ֝יִגְוְע֗וּ בִּבְלִי־דָֽעַת׃ וְֽחַנְפֵי־לֵ֭ב יָשִׂ֣ימוּ אָ֑ף"
    },
    {
      "line-num": 23,
      "range": {
        "start": "Job 36:13-mid",
        "end": "Job 36:14-mid"
      },
      "MAM-XML-fragment": "לֹ֥א יְ֝שַׁוְּע֗וּ כִּ֣י אֲסָרָֽם׃ תָּמֹ֣ת בַּנֹּ֣עַר נַפְשָׁ֑ם"
    },
    {
      "line-num": 24,
      "range": {
        "start": "Job 36:14-mid",
        "end": "Job 36:15-mid"
